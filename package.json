{
  "name": "my-frontend-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@craco/craco": "^5.6.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "apollo-boost": "^0.4.7",
    "apollo-client": "^2.6.8",
    "apollo-link-error": "^1.1.12",
    "craco-workbox": "^0.1.0",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "history": "^4.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "react-use": "^13.24.0",
    "typescript": "3.7.2",
    "yup": "^0.28.1"
  },
  "scripts": {
    "postinstall": "typesync",
    "start": "cross-env craco start",
    "build": "craco build",
    "start:with:codegen": "concurrently \"craco start\" \"yarn run generate:schema -- --watch\"",
    "generate:schema": "graphql-codegen --require dotenv/config --config codegen.yml",
    "prebuild": "yarn run generate:schema",
    "test": "craco test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@graphql-codegen/cli": "^1.12.1",
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "@graphql-codegen/typescript-react-apollo": "^1.12.1",
    "@types/concurrently": "^4.1.0",
    "@types/history": "^4.7.5",
    "@types/prettier": "^1.19.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/testing-library__jest-dom": "^5.0.1",
    "@types/testing-library__react": "^9.1.2",
    "@types/yup": "^0.26.30",
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.0",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.19.1",
    "typesync": "^0.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
